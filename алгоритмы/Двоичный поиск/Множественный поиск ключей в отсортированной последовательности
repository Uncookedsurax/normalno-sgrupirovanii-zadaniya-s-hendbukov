#include<vector>
#include <iostream>
using namespace std;

int binary_search(const vector<int>& arr, int q) {
    int left = 0, right = arr.size() - 1;

    while (left <= right) {
        int mid = left + (right - left) / 2;

        if (arr[mid] == q) {
            return mid + 1;
        }
        else if (arr[mid] < q) {
            left = mid + 1;
        }
        else {
            right = mid - 1;
        }
    }

    return -1;
}

int main()
{
	int n;
	cin >> n;
    vector<int> k(n);
    for (int i = 0; i < n; ++i) {
        cin >> k[i];
    }
    
        int j;
        cin >> j;
        vector<int>q(j);

        for (int i = 0; i < j; ++i) 
            cin >> q[i];
        
    


        for (int m = 0; m < j; ++m) {
            int result = binary_search(k, q[m]);
            if (result > 0) cout << result - 1<<endl;
            else cout << result<<endl;   
      
        }
       
}
